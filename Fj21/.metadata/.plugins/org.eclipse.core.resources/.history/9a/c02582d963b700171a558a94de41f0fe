	}
	
	public void Cadastrar(Contato contato){
		
		String sql = "insert into contatos (nome, email, endereco, dataNascimento) values (?,?,?,?)";
		
		try {
			PreparedStatement stmt = conexao.prepareStatement(sql);
		} catch (Exception e) {
			// TODO: handle exception
		}
		
	}
}
